            $.getJSON('http://foris.mybluemix.net/HelloWorld?q=webapp_token_many', function (data) {

                //Finding average readings
                var avg_moisture = 0,
                    avg_temp = 0,
                    avg_salinity = 0,
                    avg_pH = 0;

                var moisture = []; //creating moisture dictionary
                for (var i = 0; i < data.length; i++) {
                    var d1 = data[i];
                    var dattime = d1["timestamp"];  //datetime
                    dattime = new Date(Date.UTC(dattime.substring(0, 4), (dattime.substring(5, 7) - 1), dattime.substring(8, 10), dattime.substring(11, 13), dattime.substring(14, 16)));
                    var d2 = d1["sensor"];
                    var val = d2["Moisture"];
                    moisture.push({
                        x: dattime, y: val
                    });
                    avg_moisture = avg_moisture + val;
                }
                if (data.length != 0) {
                    avg_moisture = (avg_moisture / data.length);
                    document.getElementById("avg_moisture").innerHTML = avg_moisture.toFixed(2);

                }


                var temperature = []; //creating Temperature dictionary
                for (var i = 0; i < data.length; i++) {
                    var d1 = data[i];
                    var dattime = d1["timestamp"];  //datetime
                    dattime = new Date(Date.UTC(dattime.substring(0, 4), (dattime.substring(5, 7) - 1), dattime.substring(8, 10), dattime.substring(11, 13), dattime.substring(14, 16)));
                    var d2 = d1["sensor"];
                    var val = d2["Temperature"];
                    temperature.push({
                        x: dattime, y: val
                    });
                    avg_temp = avg_temp + val;
                }
                if (data.length != 0) {
                    avg_temp = (avg_temp / data.length);
                    document.getElementById("avg_temperature").innerHTML = avg_temp.toFixed(2) + " &#8457;";
                }

                var salinity = []; //creating Salinity dictionary
                for (var i = 0; i < data.length; i++) {
                    var d1 = data[i];
                    var dattime = d1["timestamp"];  //datetime
                    dattime = new Date(Date.UTC(dattime.substring(0, 4), (dattime.substring(5, 7) - 1), dattime.substring(8, 10), dattime.substring(11, 13), dattime.substring(14, 16)));
                    var d2 = d1["sensor"];
                    var val = d2["Salinity"];
                    salinity.push({
                        x: dattime, y: val
                    });
                    avg_salinity = avg_salinity + val;
                }
                if (data.length != 0) {
                    avg_salinity = (avg_salinity / data.length);
                    document.getElementById("avg_salinity").innerHTML = avg_salinity.toFixed(2);
                }

                var pH = []; //creating pH dictionary
                for (var i = 0; i < data.length; i++) {
                    var d1 = data[i];
                    var dattime = d1["timestamp"];  //datetime
                    dattime = new Date(Date.UTC(dattime.substring(0, 4), (dattime.substring(5, 7) - 1), dattime.substring(8, 10), dattime.substring(11, 13), dattime.substring(14, 16)));
                    var d2 = d1["sensor"];
                    var val = d2["Ph"];
                    pH.push({
                        x: dattime, y: val
                    });
                    avg_pH = avg_pH + val;
                }
                if (data.length != 0) {
                    avg_pH = (avg_pH / data.length);
//                    document.getElementById("avg_pH").innerHTML = avg_pH.toFixed(2);
                }

