    <script>
        var app = angular.module("forisApp", []);
        app.controller("homeCtrl", ['$scope', '$http', '$window', function( $scope,$http,$window) {
            $http({
                method: 'GET',
                url: '/checklogin'
            }).then(function (response) {
                if (response.data.status === 300) {
                    $window.location.href = '/login';
                }
                else  if(response.data.status === 200)
                {
                    $http({
                        method: 'GET',
                        url: '/profileusername'
                    }).
                    then(function(response)
                    {

                        console.log("Got profile info");
                        $scope.loginusername = angular.fromJson(response.data.result);

                    });

                    $http({
                        method: 'GET',
                        url: '/sensordata'
                    }).
                    then(function(response)
                    {
                        console.log("Got sensor info");
                        $scope.sensordata = response.data.sensor_arr;

                    });
                }

            });

            $scope.logoutsession= function ()
            {
                $http({
                    method: 'GET',
                    url: '/logoutsession'
                }).
                then(function(response)
                {
                    if(response.data.status===300)
                    {
                        console.log("destroy session");
                    }else
                    {
                        $window.location.href='/';
                    }
                });
            }


        }]);

    </script>

